-- -----------------------------------------------------
-- Table `mydb`.`det_usuario_acceso`
-- -----------------------------------------------------
DROP TABLE IF EXISTS `mydb`.`det_usuario_acceso` ;

SHOW WARNINGS;
CREATE  TABLE IF NOT EXISTS `mydb`.`det_usuario_acceso` (
  `vCLAVE` VARCHAR(45) NULL ,
  `fk_iCI` INT NOT NULL ,
  `fk_iID_ROL` INT NOT NULL ,
  `fk_iID_FUNCION` INT NOT NULL ,
  CONSTRAINT `ID_ROL`
    FOREIGN KEY (`fk_iID_ROL` )
    REFERENCES `mydb`.`gen_rol` (`pk_iID_ROL` )
    ON DELETE NO ACTION
    ON UPDATE NO ACTION,
  CONSTRAINT `CI`
    FOREIGN KEY (`fk_iCI` )
    REFERENCES `mydb`.`tbdet_usuario_datos` (`pk_iCI` )
    ON DELETE NO ACTION
    ON UPDATE NO ACTION,
  CONSTRAINT `ID_ESTATUS`
    FOREIGN KEY (`fk_iID_FUNCION` )
    REFERENCES `mydb`.`gen_estatus_registro_usu` (`pk_iID_ESTATUS` )
    ON DELETE NO ACTION
    ON UPDATE NO ACTION)
ENGINE = InnoDB;

SHOW WARNINGS;
CREATE INDEX `ID_ROL_idx` ON `mydb`.`det_usuario_acceso` (`fk_iID_ROL` ASC) ;

SHOW WARNINGS;
