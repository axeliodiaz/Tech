SHOW WARNINGS;
USE `mydb` ;

-- -----------------------------------------------------
-- Table `mydb`.`tbdet_usuario_datos`
-- -----------------------------------------------------
DROP TABLE IF EXISTS `mydb`.`tbdet_usuario_datos` ;

SHOW WARNINGS;
CREATE  TABLE IF NOT EXISTS `mydb`.`tbdet_usuario_datos` (
  `pk_iCI` INT NOT NULL ,
  `vTELF_LOCAL` VARCHAR(15) NOT NULL ,
  `vTELF_MOVIL` VARCHAR(15) NOT NULL ,
  `vSUCURSAL` VARCHAR(15) NOT NULL ,
  `vCARGO` VARCHAR(15) NOT NULL ,
  `vDEPARTAMENTO` VARCHAR(20) NOT NULL ,
  `vCORREO_EMAIL` VARCHAR(20) NOT NULL ,
  PRIMARY KEY (`pk_iCI`) )
ENGINE = InnoDB;

SHOW WARNINGS;

