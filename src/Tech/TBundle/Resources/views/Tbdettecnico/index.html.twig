{% extends '::layout.html.twig' %}

{% block content %}
    <h1>Listado de Cotizaciones</h1>
      <div class="container" >

    <section class="register">
<table class="records_list">
        <thead>
            <tr>
            <td align="center"  width="80"><h4 ><strong>Alias</strong></h4></td>
            <td width="40"></td>
            <td align="center"  width="80"><h4 ><strong>Usuario</strong></h4></td>
            <td width="40"></td>
            <td align="center"  width="80"><h4 ><strong>Acciones</strong></h4></td>
            <td width="40"></td>
            </tr>
        </thead>
        <tbody>
        {% for entity in entities %}
            <tr>
                <td align="center"  width="80"><a href="{{ path('Tecnico_show', { 'id': entity.id }) }}">{{ entity.valias  }}</a></td>
                <td width="40"></td>
                <td align="center"  width="80">{{ entity.fkIidUsuadatostecn  }}</td>
                <td width="40"></td>
                <td align="center"  width="80">
                <ul>
                    <li>
                     <a href="{{ path('Tecnico_edit', { 'id': entity.id }) }}"><img width="40" height="40"  title="Editar" src="/bundles/techt/images/Draft.png"></a>
                    </li>
                </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>
</section>
      </div>
        <ul>
        <li>
            <a href="{{ path('Tecnico_new') }}">
                <h4>Crear Tecnico</h4>
            </a>
        </li>
    </ul>
    {% endblock %}
