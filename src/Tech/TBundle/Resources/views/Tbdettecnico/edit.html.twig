{% extends '::layout.html.twig' %}

{% block content %}
    <h1>Modificar Tecnico</h1>
  <div class="container" >

    <section class="register">


    <form action="{{ path('Tecnico_update',{ 'id': entity.id }) }}" method="post" id="login">
 {{ form_start(edit_form) }}
        {{ form_errors(edit_form) }}
        <table>
            <tr>
            <td>
        <div class="error">
            {{ form_errors(edit_form.valias) }}</div>       
        <table>  <td width="80">
        {{ form_label(edit_form.valias, 'Alias:', {'required': 'true'}) }}</td>
            <td>{{ form_widget(edit_form.valias) }}</td>
        </table>
        </td>
        <td width='20px'></td>
        <td>
        <div class="error">
            {{ form_errors(edit_form.fkIidUsuaDatostecn) }}</div>       
        <table>  <td width="100">
        {{ form_label(edit_form.fkIidUsuaDatostecn, 'Usuario:', {'required': 'true'}) }}</td>
            <td>{{ form_widget(edit_form.fkIidUsuaDatostecn) }}</td>
        </table>
        </td>
        
        </tr>
        </table>
        <div style="display: none;">
        {{ form_row(edit_form.submit, { 'label': 'Actualizar' }) }}
        {{ form_row(delete_form.submit, { 'label': 'Eliminar' }) }}
        </div>

    
        <p class="submit">   
                <input type="submit" name="commit" value="Guardar">
            </p>
        
    
   {{ form_end(edit_form) }}     

</form>
</section>
</div>

   <ul class="record_actions">
    <li>
        <a href="{{ path('Tecnico') }}">
            <h4>  Volver Listado de Tecnicos</h4>
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
