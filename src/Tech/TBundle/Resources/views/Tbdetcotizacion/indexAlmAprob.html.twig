                </thead>

            </table>
            <div id="accordion">    
                {% for  key,cot in cotizaciones%}

                    {% for  key2,proyectos in cot%}

                        {% if key2=='uno'%}
                            <div>
                                <table>
                                    <thead>
                                        <tr bgcolor="#EEEEEE">
                                            
                                            <td height="25" align="center"  width="160">Código Equipos Cotizados</td>
                                            <td width="20"></td>
                                            <td align="center"  width="80">Cantidad Cotizada</td>
                                            <td width="20"></td>
                                            <td align="center"  width="160">Código Equipos a Entregar</td>
                                            <td width="20"></td>
                                            <td align="center"  width="140">Pendiente</td>
                                            <td width="20"></td>
                                            <td align="center"  width="140">Disponible</td>
                                            <td width="20"></td>
                                            <td align="center"  width="140">Entregado</td>
                                            <td width="20"></td>
                                            <td align="center"  width="100">Estatus</td>
                                            <td width="20"></td>
                                            
                                        </tr>
                                    </thead>
                                </table>

                                {% for  clave,proyecto  in proyectos%}

                                    <table>
                                        <tr>
                                            
                                            <td height="25" align="center"  width="160">{{ proyecto.icodproyecto  }}</td>
                                            <td width="20"></td>
                                            <td align="center"  width="80">{{ proyecto.icantidad }}</td>
                                            <td width="20"></td>
                                            <td align="center"  width="160">{{ proyecto.icodproyecto|replace({'S': 'V'})  }}</td>
                                     <td width="20"></td>
                                            <td align="center"  width="80">{{ (proyecto.icantidad-(proyecto.icantidaddisponible+proyecto.icantidadentregada+proyecto.icantidadrecibida)|abs)}}</td>
                                            <td width="20"></td>
                                            <td align="center"  width="140">{{ proyecto.icantidaddisponible}}</td>
                                            <td width="20"></td>
                                            <td align="center"  width="140">{{ proyecto.icantidadentregada+proyecto.icantidadrecibida}}</td>
                                            <td width="20"></td>
                                            <td align="center"  width="100">{{ proyecto.fkTbdetestatusproyecto }}</td>
                                            <td width="20"></td>-
                                            
                                           
                                            
                                        </tr>
                                    </table>

                                {%endfor%}
                            </div>
                        {%else%}
                            {% if key2=='dos'%}
                            <table>
                                  <tr>

                                    <td align="left"  width="70">{% if cot['dos'].dfecha %}{{ cot['dos'].dfecha|date('d-m-Y') }}{% endif %}</td>
                                    <td width="15"></td>
                                    
                                    <td align="left"  width="90"><a href="{{ path('Cotizacion_show', { 'id': cot['dos'].id }) }}">{{cot['dos'].codcotizacion}}</a></td>
                                    <td width="15"></td>
                                    <td align="left"  width="90"><a href="{{ path('Cotizacion_show', { 'id': cot['dos'].id }) }}">{{cot['dos'].codcotizacion}}</a></td>
                                    <td width="15"></td>
                                    <td align="left"  width="70">{% if cot['dos'].dfecha %}{{ cot['dos'].dfecha|date('d-m-Y') }}{% endif %}</td>
                                    <td width="15"></td>
                                    <td align="left"  width="70">{% if cot['dos'].dfecha %}{{ cot['dos'].dfecha|date('d-m-Y') }}{% endif %}</td>
                                    <td width="15"></td>
                                    <td align="left"  width="70">{% if cot['dos'].dfecha %}{{ cot['dos'].dfecha|date('d-m-Y') }}{% endif %}</td>
                                    <td width="15"></td>
                                    <td align="left"  width="80">{{ cot['dos'].fkiidestatusinstalacion.vdescripcion }}</td>
                                    <td width="15"></td>
                                    <td align="left"  width="90">{% if cot['dos'].tbdetliderpmo %}{{ cot['dos'].tbdetliderpmo.valias}} {%else%} 'No Asignado' {%endif%}</td>
                                    <td width="15"></td>
                                    <td align="left"  width="70">{% if cot['tres'].fkIidTbdettecnico%}{{ cot['tres'].fkIidTbdettecnico.valias}} {%else%} 'No Asignado' {%endif%}</td>
                                    <td width="15"></td>
                                    <td align="left"  width="50">5</td>
                                </tr>
                            </table>
                            {%endif%}   
                        {%endif%}

                    {%endfor%}

                {%endfor%}
            </div>  



            </setion>
    </div>

    <script>
                                                       function confirmCot(options) {
                                                           $("#d" + options.idCot).dialog({
                                                               resizable: false,
                                                               height: 350,
                                                               width: 400,
                                                               modal: true,
                                                               title: 'Confirmación de Existencia',
                                                               buttons: {
                                                                   "No": function () {
                                                                       $(this).dialog("close");
                                                                   },
                                                                   Si: function () {
                                                                       var data = {
                                                                           idCot: options.idCot
                                                                       };
                                                                       $.ajax({
                                                                           type: 'post',
                                                                           url: '{{ path("confirmCotizacion") }}',
                                                                           data: data,
                                                                           dataType: 'json',
                                                                           success: function (data) {
                                                                               if (data.error !== undefined && data.error !== '') {
                                                                                   alert(data.error);
                                                                               } else {
                                                                                   $("#d" + options.idCot).dialog("close");
                                                                               }
                                                                           },
                                                                           error: function (e, ts, et) {
                                                                               alert(ts);
                                                                           }
                                                                       });
                                                                       //
                                                                   }
                                                               }
                                                           });
                                                       }
    </script>



    <script>
        $(function () {
            $("#accordion").accordion({
                active: false,
                collapsible: true,
                heightStyle: "content",
                navigation: true,
            });
        });
        
         $('#accordion input[type="checkbox"]').click(function(e) {
                e.stopPropagation();
            });
            $("a").click(function(event){ event.stopPropagation(); });
    </script>
{% endblock %}
