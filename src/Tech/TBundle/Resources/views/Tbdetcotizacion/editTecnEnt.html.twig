{% extends '::layout.html.twig' %}

{% block content -%}
    <h1>Confirmación de Retiro de Equipos de Almacén </h1>

    <div class="container" >

        <section class="register">
            <form action="{{ path('Cotizacion_updateTecnEnt',{ 'id': entity.id }) }}" method="post" id="login">

            <table class="records_list">
                <thead>
                    <tr>
                     <td width="30"></td>
                        <td align="left"  width="75"><h4 ><strong>Fecha Notificación</strong></h4></td>
                        <td width="15"></td>
                        <td align="left"  width="70"><h4 ><strong>Número cotización</strong></h4></td>
                        <td width="15"></td>
                        <td align="left"  width="70"><h4 ><strong>Cotización Equipos</strong></h4></td>
                        <td width="15"></td>
                        <td align="left"  width="70"><h4 ><strong>Cotización Instalación</strong></h4></td>
                        <td width="15"></td>
                        <td align="left"  width="70"><h4 ><strong>Fecha Asignación</strong></h4></td>
                        <td width="15"></td>
                        <td align="left"  width="70"><h4 ><strong>Fecha Inicio</strong></h4></td>
                        <td width="15"></td>
                        <td align="left"  width="70"><h4 ><strong>Fecha Fin</strong></h4></td>
                        <td width="15"></td>
                        <td align="left"  width="80"><h4 ><strong>Estatus Proyecto</strong></h4></td>
                        <td width="15"></td>
                        <td align="left"  width="90"><h4 ><strong>Lider PMO Asignado</strong></h4></td>
                        <td width="15"></td>
                        <td align="left"  width="50"><h4 ><strong>Prioridad</strong></h4></td>
                        
                    </tr>
                </thead>

            </table>
            <div id="accordion">    
                {% for  key,cot in cotizaciones%}

                    {% for  key2,proyectos in cot%}

                        {% if key2=='uno'%}
                            <div>
                                <table>
                                    <thead>
                                        <tr bgcolor="#EEEEEE">

                                            <td height="25" align="center"  width="130">Código Equipo Cotizado</td>
                                            <td width="20"></td>
                                            <td align="center"  width="130">Código Equipo</td>
                                            <td width="20"></td>
                                            <td align="center"  width="80">Estatus Almacén</td>
                                            <td width="20"></td>
                                            <td bgcolor="#FF5F55" align="center"  width="80">Cotizado</td>
                                            <td width="20"></td>
                                            <td bgcolor="#0BD4FF" align="center"  width="80">Instalado</td>
                                            <td width="20"></td>
                                            <td bgcolor="#EB6E00" align="center"  width="80">Entregado </td>
                                            <td width="20"></td>
                                            <td bgcolor="#61EB00" align="center"  width="80">Preparado</td>
                                            <td width="20"></td>
                                            <td bgcolor="#FFFF40" align="center"  width="80">Por Entregar</td>
                                            
                                            <td width="20"></td>
                                            <td align="center"  width="80">Cantidad Retirada</td>
                                        </tr>
                                    </thead>
                                </table>

                                {% for  clave,proyecto  in proyectos%}

                                    <table>

                                        <tr>

                                            <td align="center"  width="130">{{ proyecto.icodproyecto  }}</td>
                                            <td width="20"></td>
                                            <td align="center"  width="130">{{ proyecto.icodproyecto|replace({'S': 'V'})  }}</td>
                                            <td width="20"></td>
                                            <td align="center"  width="80">{{ proyecto.fkTbdetestatusproyecto.vdescripcion}}</td>
                                            <td width="20"></td>
                                            <td align="center"  width="80">{{ proyecto.icantidad }}</td>
                                            <td width="20"></td>
                                            <td align="center"  width="80">{{ (proyecto.icantidadentregada)}}</td>
                                            <td width="20"></td>
                                            <td align="center"  width="80">{{ proyecto.icantidadrecibida}}</td>
                                            <td width="20"></td>
                                            <td bgcolor="#EEEEEE" align="center"  width="80">{{ proyecto.icantidaddisponible}}</td>
                                            <td width="20"></td>
                                            <td align="center"  width="80">{{ ((proyecto.icantidad-proyecto.icantidadrecibida)|abs)}}</td>
                                           
                                            <td width="20"></td>
                                            <td align="center" width="80"><input  min=0  style='alignment-adjust: center; width: 55px;' title="Existencia de Equipo en Almacén" 
                                                                                  type="number" name= "p_{{proyecto.id}}" value= "{{proyecto.icantidaddisponible|abs}}"
                                                                                  max={{proyecto.icantidaddisponible|abs}}><br>

                                            </td>







                                        </tr>
                                    </table>

                                {%endfor%}
                            </div>
                        {%else%}
                            {% if key2=='dos'%}
                                <table>
                                    <tr>

                                            <tr>

                                    <td align="left"  width="70">{% if cot['dos'].dfecha %}{{ cot['dos'].dfecha|date('d-m-Y') }}{% endif %}</td>
                                    <td width="15"></td>
                                    
                                    <td align="left"  width="70"><a href="{{ path('Cotizacion_show', { 'id': cot['dos'].id }) }}">{{cot['dos'].codcotizacion}}</a></td>
                                    <td width="15"></td>
                                    <td align="left"  width="70"><a href="{{ path('Cotizacion_show', { 'id': cot['dos'].id }) }}">{{cot['dos'].codcotizacion}}</a></td>
                                    <td width="15"></td>
                                    <td align="left"  width="70"><a href="{{ path('Cotizacion_show', { 'id': cot['dos'].id }) }}">{{cot['dos'].codcotizacion}}</a></td>
                                    <td width="15"></td>
                                    <td align="left"  width="70">{% if cot['dos'].dfecha %}{{ cot['dos'].dfecha|date('d-m-Y') }}{% endif %}</td>
                                    <td width="15"></td>
                                    <td align="left"  width="70">{% if cot['dos'].dfechaIni %}{{ cot['dos'].dfechaIni|date('d-m-Y') }}{% endif %}</td>
                                    <td width="15"></td>
                                    <td align="left"  width="70">{% if cot['dos'].dfechaFin %}{{ cot['dos'].dfechaFin|date('d-m-Y') }}{% endif %}</td>
                                    <td width="15"></td>
                                    <td align="left"  width="80">{{ cot['dos'].fkiidestatusinstalacion.vdescripcion }}</td>
                                    <td width="15"></td>
                                    <td align="left"  width="90">{% if cot['dos'].tbdetliderpmo %}{{ cot['dos'].tbdetliderpmo.valias}} {%else%} 'No Asignado' {%endif%}</td>
                                    <td width="15"></td>
                                    <td align="left"  width="50">{% if cot['dos'].iprioridad%}{{ cot['dos'].iprioridad}} {%else%} 'No Asignado' {%endif%}</td>
                                
                                    </tr>
                                </table>
                            {%endif%}   
                        {%endif%}

                    {%endfor%}

                {%endfor%}
            </div>  


            <br>
            
            <p class="submit">   
                <input  type="submit" name="commit" value="Confirmar">
            </p>
            </form>
            
        </section>
    </div>
    <script>
        $(function () {
            $("#accordion").accordion({
                active: false,
                collapsible: false,
                heightStyle: "content",
                navigation: false,
            });
        });

    </script>
{% endblock %}
