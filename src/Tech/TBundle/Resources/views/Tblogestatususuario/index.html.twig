{% extends '::layout.html.twig' %}

{% block content %}
    <h1>Listado de Estatus Asociados a Usuarios</h1>
<br><br>
    <table class="records_list">
        <thead>
            <tr>
                
                <td>Fecha de Cambio</td>
                <td>Acciones</td>
            </tr>
        </thead>
        <tbody>
        {% for entity in entities %}
            <tr>
               
                <td>{% if entity.dfechaCambio %}{{ entity.dfechaCambio|date('Y-m-d H:i:s') }}{% endif %}</td>
                <td>
                <ul>
                    <li>
                        <a href="{{ path('RegistroEstatus_show', { 'id': entity.id }) }}">Ver</a>
                    </li>
                    <li>
                        <a href="{{ path('RegistroEstatus_edit', { 'id': entity.id }) }}">Editar</a>
                    </li>
                </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

        <ul>
        <li>
            <a href="{{ path('RegistroEstatus_new') }}">
              Crear nuevo Cambio.
            </a>
        </li>
    </ul>
    {% endblock %}
