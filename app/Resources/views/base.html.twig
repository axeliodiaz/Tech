<!DOCTYPE html>
<html>
    <head>
        <title>{% block title %}{% endblock %}</title>
        <link href="{{ asset('bundles/techt/css/structure.css') }}" rel="stylesheet" type="text/css" media="all" />
        <link href="{{ asset('bundles/techt/css/custom-style.css') }}" rel="stylesheet" type="text/css" media="screen" />
        <link href="{{ asset('bundles/techt/css/body.css') }}" rel="stylesheet" type="text/css" media="all" />
        <link href="{{ asset('bundles/techt/css/style.css') }}" rel="stylesheet" type="text/css" media="screen" />
        <link href="{{ asset('bundles/techt/css/styleform.css') }}" rel="stylesheet" type="text/css" media="screen" />
        <script src="{{ asset('bundles/techt/js/jquery-2.1.0.min.js') }}" type="text/javascript"></script>
        <link href="{{ asset('bundles/techt/css/rating.css') }}" rel="stylesheet" type="text/css" media="screen" />
        <link href='http://fonts.googleapis.com/css?family=Open+Sans+Condensed:300' rel='stylesheet' type='text/css'>
        <script src="{{ asset('bundles/techt/js/rate.js') }}" type="text/javascript"></script>
        <script src="{{ asset('bundles/techt/js/jquery-1.4.2.min.js') }}" type="text/javascript"></script>




        {% block head %}
        {% endblock %}
    </head>
    <body>
        <br>
<div class="container">

    <!-- Push Wrapper -->
    <div class="mp-pusher mp-pushed" id="mp-pusher" style="-webkit-transform: translate3d(300px, 0px, 0px);">
        <!-- mp-menu -->
        <nav id="mp-menu" class="mp-menu mp-overlap">

            <div class="mp-level mp-level-open" data-level="1">
                <h2 ><a href="{{path('index')}}">Inicio</a></h2>
                <ul>

                    {% if (app.session.get('usuario_rol')!="Empleado") and 
                        (app.session.get('usuario_rol')!="Cliente") %}
                    <li class="icon icon-arrow-left">
                        <a class="icon icon-display" href="#">Clientes</a>
                        <div class="mp-level" data-level="2" style="-webkit-transform: translate3d(-100%, 0px, 0px) translate3d(-40px, 0px, 0px);">
                            <h2 class="icon icon-display">Clentes</h2>
                            <a class="mp-back" href="#">Atrás</a>
                            <ul>
                                <li class="icon icon-arrow-left">
                                    <a class="icon icon-phone" href="{{ path('Registro_new') }}">Solicitud de Registro</a>

                                </li>

                            </ul>
                        </div>
                    </li>
                {% endif %}

                {% if (app.session.get('usuario_rol')=="Empleado") or 
                      (app.session.get('usuario_rol')=="Cliente") %}



                    <li class="icon icon-arrow-left"> 
                        <a class="icon icon-display" href="#">Clientes</a>
                        <div class="mp-level" data-level="2" style="-webkit-transform: translate3d(-100%, 0px, 0px) translate3d(-40px, 0px, 0px);">
                            <h2 class="icon icon-display">Clentes</h2>
                            <a class="mp-back" href="#">Atrás</a>
                            <ul>
                                {% if app.session.get('usuario_rol')=="Cliente" %}

                                <li class="icon icon-arrow-left">
                                    <a class="icon icon-phone" href="{{ path('Registro_new') }}">Solicitud de Registro</a>

                                </li>
                                {% endif %}
                                {% if app.session.get('usuario_rol')=="Empleado" %}
                                <li class="icon icon-arrow-left">
                                    <a class="icon icon-phone" href="{{ path('Registro_new') }}">Solicitud de Registro</a>

                                </li>
                                <li class="icon icon-arrow-left">
                                    <a class="icon icon-tv" href="{{ path('Registro') }}">Listado de Solicitudes de Registro</a>

                                </li>

                                {% endif %}

                            </ul>
                        </div>
                    </li>
                    {% endif %}
                    {% if (app.session.get('usuario_rol')=="Cliente") or (app.session.get('usuario_rol')=="Empleado") %}
                    <li class="icon icon-arrow-left">
                        <a class="icon icon-news" href="#">Servicios</a>
                        <div class="mp-level" data-level="2">
                            <h2 class="icon icon-news">Servicios</h2>
                            <a class="mp-back" href="#">Atras</a>
                            <ul>
                                <li><a href="{{ path('SolicitudServicio_new') }}">Solicitud de Servicio</a></li>
                                <li><a href="{{ path('SolicitudServicio') }}">Consulta de Solicitud de Servicios</a></li> </ul>
                        </div>
                    </li>
                    {% endif %}
                    {% if (app.session.get('usuario_rol')=="Empleado") %}
                    <li class="icon icon-arrow-left">
                        <a class="icon icon-shop" href="#">Contratos</a>
                        <div class="mp-level" data-level="2" style="-webkit-transform: translate3d(-100%, 0px, 0px) translate3d(-40px, 0px, 0px);">
                            <h2 class="icon icon-shop">Contratos</h2>
                            <a class="mp-back" href="#">Atras</a>
                            <ul>
                                <li class="icon icon-arrow-left">
                                    <a class="icon icon-t-shirt" href="{{ path('Contratos') }}">Listado</a>

                                </li>



                            </ul>
                        </div>
                    </li>
                    
                    <li><a href="{{ path('Empresa') }}">Empresas</a></li>
                    {% endif %}
                </ul>

            </div>
        </nav>
        <!-- /mp-menu -->

        <div class="scroller"><!-- this is for emulating position fixed of the nav -->
            <div class="scroller-inner">
                <!-- Top Navigation -->
                <div class="content clearfix">

                    <div class="block block-40 clearfix">
                                                 {% block body %}
                                               {% endblock %}                      

                        <p><a href="#" id="trigger" ></a></p>			
                    </div>
                </div>
            </div><!-- /scroller-inner -->
        </div><!-- /scroller -->

    </div><!-- /pusher -->
</div><!-- /container -->
        <script src="{{ asset('bundles/techt/js/classie.js') }}" type="text/javascript"></script>
        <script src="{{ asset('bundles/techt/js/mlpushmenu.js') }}" type="text/javascript"></script>

        <link href="{{ asset('bundles/techt/css/normalize.css') }}" rel="stylesheet" type="text/css" media="screen" />
        <link href="{{ asset('bundles/techt/css/demo_1.css') }}" rel="stylesheet" type="text/css" media="screen" />

        <link href="{{ asset('bundles/techt/css/component.css') }}" rel="stylesheet" type="text/css" media="screen" />
        <script src="{{ asset('bundles/techt/js/modernizr.custom.js') }}" type="text/javascript"></script>
        <script src="{{ asset('bundles/techt/js/ga.js') }}" type="text/javascript"></script>


        <script src="{{ asset('bundles/techt/js/ad.js') }}" type="text/javascript"></script>


        <script>
            new mlPushMenu(document.getElementById('mp-menu'), document.getElementById('trigger'));
        </script>